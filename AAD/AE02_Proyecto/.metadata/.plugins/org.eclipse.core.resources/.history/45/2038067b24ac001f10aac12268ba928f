package model;

public class User {
	private String login;
	private String passwordHash;
	private String type;

	public User(String login, String passwordHash, String type) {
		this.login = login;
		this.passwordHash = passwordHash;
		this.type = type;
	}

	public String getLogin() {
		return login;
	}

	public String getPasswordHash() {
		return passwordHash;
	}

	public String getType() {
		return type;
	}
}
