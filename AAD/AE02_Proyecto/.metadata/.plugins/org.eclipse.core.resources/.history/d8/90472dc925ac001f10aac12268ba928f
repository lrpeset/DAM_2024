package model;

public class User {
	private int id;
	private String login;
	private String passwordHash;
	private String type;

	public User(int id, String login, String passwordHash, String type) {
		this.id = id;
		this.login = login;
		this.passwordHash = passwordHash;
		this.type = type;
	}
	
	public int getId() {
		return id;
	}

	public String getLogin() {
		return login;
	}

	public String getPasswordHash() {
		return passwordHash;
	}

	public String getType() {
		return type;
	}
}
