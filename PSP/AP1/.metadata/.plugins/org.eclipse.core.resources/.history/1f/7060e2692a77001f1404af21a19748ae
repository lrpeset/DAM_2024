package ejercicios;

import java.util.Scanner;

public class Ejercicio16 {
	public static void main(String[] args) {
		
		try(Scanner teclado = new Scanner(System.in)) {
			System.out.print("Vamos a descubrir tu número de la suerte.");
			System.out.print("Escribe el dia de tu nacimiento (DD): ");
			int dia = teclado.nextInt();
			
			System.out.print("Escribe el mes de tu nacimiento (MM): ");
			int mes = teclado.nextInt();
			
			System.out.print("Escribe el año de tu nacimiento (AA): ");
			int año = teclado.nextInt();
		}
	}
}
