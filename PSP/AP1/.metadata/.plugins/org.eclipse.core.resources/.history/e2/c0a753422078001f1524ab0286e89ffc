package ejercicios;

public class Ejercicio28 {

	public class Vehículo {
		String tipo;
		String marca;
		String modelo;

		public Vehículo(String tipo, String marca, String modelo) {
			this.tipo = tipo;
			this.marca = marca;
			this.modelo = modelo;
		}
		
		public void mostrarInfo() {
			System.out.print("Tipo: " + tipo, "Marca: " + marca, "Modelo: " + modelo);
		}
	}
}
