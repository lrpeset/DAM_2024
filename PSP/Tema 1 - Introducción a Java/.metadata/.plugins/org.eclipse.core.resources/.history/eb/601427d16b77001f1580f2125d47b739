package ejercicios;

import java.util.Random;

public class Ejercicio24 {

	public static void main(String[] args) {

		Random randomFirstNumber = new Random();
		Random randomSecondNumber = new Random();

		int firstNumber = randomFirstNumber.nextInt(11);
		int secondNumber = randomSecondNumber.nextInt(11);

		if (firstNumber == secondNumber) {
			System.out.println("¡Enhorabuena! ambos números coinciden así que has ganado un caramelo.");
		}
	}
}